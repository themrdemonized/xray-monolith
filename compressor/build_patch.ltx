[options] ; exclude files from compression with such extension
exclude_exts = *.ncb,*.sln,*.vcproj,*.old,*.rc,*.scc,*.vssscc,*.bmp,*.exe,*.cmd,*.bat,*.db,*.xdb,*.bak*,*.bmp,*.smf,*.uvm,*.prj,*.tga,*.txt,*.rtf,*.doc,*.log,*.*~*,*~*.*,*.rar,*.sfk,*.tmp

[include_folders]
.\			= true ; корень
;ai			= true
;anims 		= true
;configs		= true
;levels 		= true
;meshes 		= true
;scripts 	= true 
;shaders 	= true
;sounds 		= true
;textures 	= true

[header]
auto_load = true
level_name = anomaly ; former level name, now can be mod name
level_ver = 1.8 ; former level version, now can be mod version
entry_point = $fs_root$\gamedata\ ; do not change !
creator = "Modder" ; creator's name
link = "www.moddb.com/mods/stalker-anomaly" ; creator's link